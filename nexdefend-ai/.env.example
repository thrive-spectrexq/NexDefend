# nexdefend-ai/.env.example

# Comma-separated list of allowed CORS origins
CORS_ALLOWED_ORIGINS=

# URL for the main Go API (REQUIRED)
GO_API_URL=http://api:8080/api/v1
CORE_API_URL=http://api:8080/api/v1

# Secret token for authenticating with other services (REQUIRED)
AI_SERVICE_TOKEN=

# Kafka broker address
KAFKA_BROKER=

# PostgreSQL Database
DB_NAME=
DB_USER=
DB_PASSWORD=
DB_HOST=
DB_PORT=
